  "views": {
    "public.accounts_due_for_update_view": {
      "name": "accounts_due_for_update_view",
      "schema": "public",
      "columns": {
        "id": {
          "name": "id",
          "type": "uuid",
          "primaryKey": true,
          "notNull": true
        },
        "user_id": {
          "name": "user_id",
          "type": "uuid",
          "primaryKey": false,
          "notNull": true
        },
        "provider": {
          "name": "provider",
          "type": "connected_account_provider",
          "typeSchema": "public",
          "primaryKey": false,
          "notNull": true
        },
        "account_id": {
          "name": "account_id",
          "type": "text",
          "primaryKey": false,
          "notNull": true
        },
        "access_token": {
          "name": "access_token",
          "type": "text",
          "primaryKey": false,
          "notNull": true
        },
        "refresh_token": {
          "name": "refresh_token",
          "type": "text",
          "primaryKey": false,
          "notNull": false
        },
        "expires_at": {
          "name": "expires_at",
          "type": "timestamp",
          "primaryKey": false,
          "notNull": false
        },
        "updated_at": {
          "name": "updated_at",
          "type": "timestamp",
          "primaryKey": false,
          "notNull": false
        }
      },
      "definition": "\n  SELECT \n    \"connected_accounts\".\"id\",\n    \"connected_accounts\".\"user_id\",\n    \"connected_accounts\".\"provider\",\n    \"connected_accounts\".\"account_id\",\n    \"connected_accounts\".\"access_token\",\n    \"connected_accounts\".\"refresh_token\",\n    \"connected_accounts\".\"expires_at\",\n    \"connected_accounts\".\"updated_at\"\n  FROM \"connected_accounts\"\n  JOIN \"profiles\" ON \"connected_accounts\".\"user_id\" = \"profiles\".\"id\"\n  LEFT JOIN \"subscriptions\" ON \"profiles\".\"id\" = \"subscriptions\".\"user_id\"\n  WHERE \n    -- 1. Annual Pro: Update if older than 15 minutes\n    (\n      \"profiles\".\"tier\" = 'pro' \n      AND \"subscriptions\".\"interval\" = 'year'\n      AND (\n        \"connected_accounts\".\"updated_at\" < NOW() - INTERVAL '15 minutes' \n        OR \"connected_accounts\".\"updated_at\" IS NULL\n      )\n    )\n    OR\n    -- 2. Monthly Pro: Update if older than 1 hour\n    (\n      \"profiles\".\"tier\" = 'pro' \n      AND (\"subscriptions\".\"interval\" = 'month' OR \"subscriptions\".\"interval\" IS NULL)\n      AND (\n        \"connected_accounts\".\"updated_at\" < NOW() - INTERVAL '1 hour' \n        OR \"connected_accounts\".\"updated_at\" IS NULL\n      )\n    )\n    OR\n    -- 3. Free Users: Update if older than 24 hours\n    (\n      \"profiles\".\"tier\" = 'free' \n      AND (\n        \"connected_accounts\".\"updated_at\" < NOW() - INTERVAL '24 hours' \n        OR \"connected_accounts\".\"updated_at\" IS NULL\n      )\n    )\n",
      "materialized": false,
      "with": {
        "securityInvoker": true
      },
      "isExisting": false
    }
  },
